<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="admin/tool/password/db" VERSION="20190628" COMMENT="XMLDB file for Moodle admin/tool/password"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="tool_password" COMMENT="Table for storing password lockout information">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="changetime" TYPE="int" LENGTH="12" NOTNULL="false" DEFAULT="0" SEQUENCE="false" COMMENT="Time the users password was last changed"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>